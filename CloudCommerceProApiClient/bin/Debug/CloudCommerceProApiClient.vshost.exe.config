<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <!-- YOU CAN GET THESE SETTINGS FROM AuthDetails.txt and PublicKey.xml in your Api Start Kit -->
    <add key="BrandID" value="{YOUR BRAND ID}"/>
    <add key="PublicKeyModulus" value="{YOUR PUBLIC KEY}"/>
    <add key="PublicKeyExponent" value="{YOUR EXPONENT}"/>
    <add key="Password" value="{YOUR PASSWORD}"/>
  </appSettings>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICCPApiSalesChannelService" />
                <binding name="BasicHttpBinding_ICCPAPIProductsService" />
                <binding name="BasicHttpsBinding_ICCPAPIProductsService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpsBinding_ICCPApiSalesChannelService">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
         
            <endpoint address="https://wcfccpservicesbase.cloudcommercepro.com/CCPApiProductsService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPAPIProductsService"
                contract="CloudCommercePro.Products.ICCPAPIProductsService"
                name="BasicHttpsBinding_ICCPAPIProductsService" />
          
            <endpoint address="https://wcfccpservicesbase.cloudcommercepro.com/CCPApiSalesChannelService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPApiSalesChannelService"
                contract="CloudCommercePro.SalesChannels.ICCPApiSalesChannelService"
                name="BasicHttpsBinding_ICCPApiSalesChannelService" />
        </client>
    </system.serviceModel>
</configuration>