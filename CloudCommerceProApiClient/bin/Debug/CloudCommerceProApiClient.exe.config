<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <!-- YOU CAN GET THESE SETTINGS FROM AuthDetails.txt and PublicKey.xml in your Api Start Kit -->
    <add key="BrandID" value="{YOUR BRAND ID}"/>
    <add key="PublicKeyModulus" value="{YOUR PUBLIC KEY}"/>
    <add key="PublicKeyExponent" value="{YOUR EXPONENT}"/>
    <add key="Password" value="{YOUR PASSWORD}"/>
  </appSettings>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICCPApiSalesChannelService" />
                <binding name="BasicHttpBinding_ICCPAPIProductsService" />
                <binding name="BasicHttpsBinding_ICCPAPIProductsService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpsBinding_ICCPApiSalesChannelService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_CCPProductOptions" />
                <binding name="BasicHttpsBinding_CCPProductOptions">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_CCPProductRanges" />
                <binding name="BasicHttpsBinding_CCPProductRanges">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ICCPApiCustomerService" />
                <binding name="BasicHttpsBinding_ICCPApiCustomerService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ICCPApiPaymentService" />
                <binding name="BasicHttpsBinding_ICCPApiPaymentService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ICcpApiTransactionService" />
                <binding name="BasicHttpsBinding_ICcpApiTransactionService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ICCPApiOrderService" />
                <binding name="BasicHttpBinding_ICcpApiOrderReturnsService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductsService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICCPAPIProductsService"
                contract="CloudCommercePro.Products.ICCPAPIProductsService"
                name="BasicHttpBinding_ICCPAPIProductsService" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductsService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPAPIProductsService"
                contract="CloudCommercePro.Products.ICCPAPIProductsService"
                name="BasicHttpsBinding_ICCPAPIProductsService" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiSalesChannelService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICCPApiSalesChannelService"
                contract="CloudCommercePro.SalesChannels.ICCPApiSalesChannelService"
                name="BasicHttpBinding_ICCPApiSalesChannelService" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiSalesChannelService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPApiSalesChannelService"
                contract="CloudCommercePro.SalesChannels.ICCPApiSalesChannelService"
                name="BasicHttpsBinding_ICCPApiSalesChannelService" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductOptionService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_CCPProductOptions"
                contract="CloudCommercePro.ProductOptions.CCPProductOptions"
                name="BasicHttpBinding_CCPProductOptions" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductOptionService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_CCPProductOptions"
                contract="CloudCommercePro.ProductOptions.CCPProductOptions"
                name="BasicHttpsBinding_CCPProductOptions" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductRangeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_CCPProductRanges"
                contract="CloudCommercePro.ProductRanges.CCPProductRanges"
                name="BasicHttpBinding_CCPProductRanges" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiProductRangeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_CCPProductRanges"
                contract="CloudCommercePro.ProductRanges.CCPProductRanges"
                name="BasicHttpsBinding_CCPProductRanges" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiCustomerService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICCPApiCustomerService"
                contract="CloudCommercePro.Customers.ICCPApiCustomerService"
                name="BasicHttpBinding_ICCPApiCustomerService" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiCustomerService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPApiCustomerService"
                contract="CloudCommercePro.Customers.ICCPApiCustomerService"
                name="BasicHttpsBinding_ICCPApiCustomerService" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CCPApiPaymentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICCPApiPaymentService"
                contract="CloudCommercePro.Payments.ICCPApiPaymentService"
                name="BasicHttpBinding_ICCPApiPaymentService" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CCPApiPaymentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICCPApiPaymentService"
                contract="CloudCommercePro.Payments.ICCPApiPaymentService"
                name="BasicHttpsBinding_ICCPApiPaymentService" />
            <endpoint address="http://devwcfccpservicesbase.cloudcommercepro.com/CcpApiTransactionService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICcpApiTransactionService"
                contract="CloudCommercePro.Transactions.ICcpApiTransactionService"
                name="BasicHttpBinding_ICcpApiTransactionService" />
            <endpoint address="https://devwcfccpservicesbase.cloudcommercepro.com/CcpApiTransactionService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_ICcpApiTransactionService"
                contract="CloudCommercePro.Transactions.ICcpApiTransactionService"
                name="BasicHttpsBinding_ICcpApiTransactionService" />
            <endpoint address="http://ccpapi.local/CCPApiOrderService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICCPApiOrderService"
                contract="CloudCommercePro.Orders.ICCPApiOrderService" name="BasicHttpBinding_ICCPApiOrderService" />
            <endpoint address="http://ccpapi.local/CcpApiOrderReturnsService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICcpApiOrderReturnsService"
                contract="CloudCommercePro.OrderReturns.ICcpApiOrderReturnsService"
                name="BasicHttpBinding_ICcpApiOrderReturnsService" />
        </client>
    </system.serviceModel>
</configuration>